<?xml version="1.0" encoding="UTF-8"?>
<part>
	<Author>Alban Nanty</Author>
	<ImageURL>https://images.brickset.com/sets/images/60050-1.jpg</ImageURL>
	<Description>
		<en>Train Station</en>
		<fr>Gare Ferovière</fr>
	</Description>
	<SortingKey>Station</SortingKey>
	<!-- The margin of this part in stud for snaping on the grid, relative to the bitmap -->
	<SnapMargin>
		<left>0.5</left>
		<right>2</right>
		<top>0</top>
		<bottom>0</bottom>
	</SnapMargin>
	<!-- list of connexion point (assuming that this part can connect to another one), the position of the connexion is in stud coord and relative to the origine defined above) -->
	<ConnexionList>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>-32.75</x>
				<y>6.5</y>
			</position>
			<angle>180</angle>
			<angleToPrev>180</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>180</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->
			<nextConnexionPreference>1</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>1</electricPlug>
		</connexion>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>31.25</x>
				<y>6.5</y>
			</position>
			<angle>0</angle>
			<angleToPrev>180</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>180</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->
			<nextConnexionPreference>0</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>-1</electricPlug>
		</connexion>
	</ConnexionList>

	<!-- Remap info for 4DBrix format -->
	<FourDBrix>
		<PartType>BASEPLATE</PartType>
		<PartName>/structures/train/60050.svg</PartName>
	</FourDBrix>
</part>