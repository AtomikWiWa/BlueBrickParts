<?xml version="1.0" encoding="UTF-8"?>
<part>
	<Author>Alban Nanty</Author>
	<Description>
		<en>Continuous curve adapter (Radius 40 studs)</en>
		<fr>Adaptateur de rail courbe (rayon de 40 tenons)</fr>
	</Description>
	<SortingKey>A2.5</SortingKey>

	<!-- list of connexion point (assuming that this part can connect to another one), the position of the connexion is in stud coord and relative to the origine defined above) -->
	<ConnexionList>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>-3.125</x>
				<y>-0.1875</y>
			</position>
			<angle>180</angle>
			<angleToPrev>188.1</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>171.9</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>1</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>1</electricPlug>
		</connexion>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>2.5318125</x>
				<y>0.214825</y>
			</position>
			<angle>8.1</angle>
			<angleToPrev>171.9</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>188.1</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>0</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>-1</electricPlug>
		</connexion>
	</ConnexionList>

	<!-- Remap info for 4DBrix format -->
	<FourDBrix>
		<PartType>SEGMENT</PartType>
		<PartName>TS_ADAPTERCONTINUOUSCURVE</PartName>
	</FourDBrix>
</part>