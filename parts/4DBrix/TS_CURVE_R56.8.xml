<?xml version="1.0" encoding="UTF-8"?>
<part>
	<Author>Alban Nanty</Author>
	<Description>
		<en>Curve track (Radius 56 studs)</en>
		<fr>Rail courbe (rayon de 56 tenons)</fr>
	</Description>
	<SortingKey>A2.6</SortingKey>

	<!-- list of connexion point (assuming that this part can connect to another one), the position of the connexion is in stud coord and relative to the origine defined above) -->
	<ConnexionList>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>-9.1875</x>
				<y>-1.25</y>
			</position>
			<angle>180</angle>
			<angleToPrev>198</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>162</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>1</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>1</electricPlug>
		</connexion>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>8.11745</x>
				<y>1.490835</y>
			</position>
			<angle>18</angle>
			<angleToPrev>162</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>198</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>0</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>-1</electricPlug>
		</connexion>
	</ConnexionList>

	<!-- Remap info for 4DBrix format -->
	<FourDBrix>
		<PartType>SEGMENT</PartType>
		<PartName>TS_CURVE_R56</PartName>
	</FourDBrix>
</part>